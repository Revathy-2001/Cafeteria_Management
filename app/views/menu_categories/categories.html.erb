<ul class = "Navbar_users">
  <li class = "Navbar_users_contents"><a href="#">Menu Categories</a></li>
  <li class = "Navbar_users_contents"><a href="#">Your orders</a></li>
  <li class = "Navbar_users_contents"><a href="#">Menu Items</a></li>
  <li class = "Navbar_users_contents" style="float:right">   <%= link_to "Sign out",destroy_session_path,method:"delete" %></li>
  <li class = "Welcome" style="float:right">Welcome <%= @current_user.first_name %></li>
</ul>

<div class = "Categories_card_Section">
  <% MenuCategory.all.each do | category | %>
     <div class = "Categories_block">
      <p class = "Category_name"><%= category.name%></p>
       <%= link_to "View Menu Items",show_menu_items_path(id: "#{category.id}"),class:"View_item" %>
     </div>
  <% end %>
</div>
